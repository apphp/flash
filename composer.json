{
    "name": "apphp/flash",
    "description": "Flash messages for Laravel",
    "keywords": ["apphp", "laravel", "laravel-package", "laravel6-package", "laravel6", "flash messages"],
    "homepage": "https://apphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Akopyan",
            "email": "admin@apphp.com",
            "homepage": "https://apphp.com/php-framework",
            "role": "Founder and project lead"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-0": {
            "Apphp\\Flash": "src/"
        },
        "files": [
            "src/Apphp/Flash/functions.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Apphp\\Flash\\FlashServiceProvider"
            ],
            "aliases": {
                "Flash": "Apphp\\Flash\\Flash"
            }
        }
    }
}